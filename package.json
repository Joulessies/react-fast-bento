{
  "name": "react-fast-bento",
  "version": "0.1.0",
  "description": "Lightweight React components for bento grid layouts and boxes.",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "src"
  ],
  "keywords": [
    "react",
    "bento",
    "grid",
    "layout",
    "components"
  ],
  "author": "Julius",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julius/react-fast-bento-remakea.git"
  },
  "bugs": {
    "url": "https://github.com/Julius/react-fast-bento-remake/issues"
  },
  "homepage": "https://github.com/Julius/react-fast-bento-remake#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/preset-react": "^7.27.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.12.0",
    "@size-limit/preset-small-lib": "^11.1.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@vitejs/plugin-react": "^5.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "tough-cookie": "^4.1.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "size-limit": "^11.1.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "scripts": {
    "test": "vitest",
    "test:ci": "vitest run --reporter=default",
    "typecheck": "npx tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.{js,jsx}\" \"test/**/*.{js,jsx}\"",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "src/index.js",
      "import": "{ BentoGrid, BentoBox }",
      "limit": "2 kB"
    }
  ],
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  }
}
